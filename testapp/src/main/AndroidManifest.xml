<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>

    <application
        android:name=".ApplicationMain"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:theme="@style/Theme.Neuroidandroidcomposesandbox">
        <activity
            android:name=".Splash"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.neuroid.example.Instructions"/>
        <activity android:name="com.neuroid.example.AddressInformation"/>
        <activity android:name="com.neuroid.example.ApplicationSummary"/>
        <activity android:name="com.neuroid.example.ContactInformation"/>
        <activity android:name="com.neuroid.example.EmployerInformation"/>
        <activity android:name="com.neuroid.example.PersonalInformation"/>
        <activity android:name="com.neuroid.example.LoanApplication"/>
        <activity android:name="com.neuroid.example.Login"/>
        <activity android:name="com.neuroid.example.SignUp"/>
        <activity android:name="com.neuroid.example.SendMoney"/>
        <activity android:name="com.neuroid.example.PersonalInformationTwo"/>
    </application>

</manifest>